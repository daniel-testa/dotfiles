promptedGoto :: Host -> X ()\
promptedGoto = workspacePrompt myXPConfig . goto    -- (27)\
\
promptedGotoOtherScreen :: Host -> X ()\
promptedGotoOtherScreen host =\
  workspacePrompt myXPConfig $ \ws -> do            -- (27)\
    nextScreen\
    goto host ws\
\
promptedShift :: X ()\
promptedShift = workspacePrompt myXPConfig $ windows . W.shift  -- (27)\
