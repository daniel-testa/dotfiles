    -- mod-[1..],       Switch to workspace N\
    -- mod-shift-[1..], Move client to workspace N\
    -- mod-ctrl-[1..],  Switch to workspace N on other screen\
    [ (m ++ "M-" ++ [k], f i)                                   -- (0)\
        | (i, k) <- zip (XMonad.workspaces conf) "1234567890-=[]\\" -- (0)\
        , (f, m) <- [ (goto', "")                    -- (0a)\
