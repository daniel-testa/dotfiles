    -- ability to toggle between fullscreen, reflect x/y, no borders,\
    -- and mirrored.\
    mkToggle1 NBFULL $                                  -- (14)\
    mkToggle1 REFLECTX $                                -- (14,13)\
    mkToggle1 REFLECTY $                                -- (14,13)\
    mkToggle1 NOBORDERS $                               --  "\
    mkToggle1 MIRROR $                                  --  "\
\
    -- borders automatically disappear for fullscreen windows.\
    smartBorders $                                              -- (7)\
